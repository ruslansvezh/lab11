#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	setlocale(0, "");
	int a, b,i,j;
	{
		cout << "Введите A и B(A < B) : " << endl;
		cin >> a >> b;
		if (a < b)
		{
			for ( i = a; i <= b; i++)
			{
				for ( j = 1; j <= i; j++)
				{
					cout  << i<< "\t";
				}
				cout << endl;
			}
		}
		else
		{
			cout << "Повторите ввод данных!" << endl;
		}
	}
}
